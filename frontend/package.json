{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --port 4200 --aot --locale en --i18n-format xlf --i18n-file ./src/locale/messages.en.xlf",
    "start:ja": "ng serve --port 4200 --aot --locale ja --i18n-format xlf --i18n-file ./src/locale/messages.ja.xlf",
    "build:en": "ng build -vc=true --aot --prod --locale en --i18n-format xlf --i18n-file ./src/locale/messages.en.xlf --bh /en/ --output-path dist/en && rm -rf public/assets && cp -r src/assets public/assets && rm -rf public/en && mv dist/en public/en && rm -rf public/en/assets",
    "build:ja": "ng build -vc=true --aot --prod --locale ja --i18n-format xlf --i18n-file ./src/locale/messages.ja.xlf --bh /ja/ --output-path dist/ja && rm -rf public/assets && cp -r src/assets public/assets && rm -rf public/ja && mv dist/ja public/ja && rm -rf public/ja/assets",
    "build": "npm run build:en && npm run build:ja",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n-extract": "./node_modules/.bin/ng-xi18n --i18nFormat=xlf -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.3",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/flex-layout": "^5.0.0-beta.13",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "^5.2.3",
    "@angular/material-moment-adapter": "^5.2.3",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "apollo-angular": "^1.0.1",
    "apollo-angular-link-http": "^1.0.2-beta.0",
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-client": "^2.2.5",
    "apollo-link-context": "^1.0.7",
    "auth0-js": "^9.2.0",
    "auth0-lock": "^11.3.0",
    "core-js": "^2.5.3",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.7.3",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "rxjs": "^5.5.6",
    "sweetalert2": "^7.12.15",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/language-service": "^5.2.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
