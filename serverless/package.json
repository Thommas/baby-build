{
  "name": "pathofchild-serverless",
  "scripts": {
    "build": "NODE_ENV=local serverless webpack --stage local",
    "deploy": "NODE_ENV=local serverless deploy --stage local",
    "watch": "nodemon -w lib -e '.*' -x 'npm run build'",
    "start": "npm run deploy && npm run watch",
    "info": "serverless info --stage local",
    "load": "NODE_ENV=local ts-node ./lib/command/load.command.ts",
    "test": "NODE_ENV=test ts-node node_modules/jasmine/bin/jasmine \"lib/**/*.spec.ts\"",
    "e2e": "NODE_ENV=test ts-node node_modules/jasmine/bin/jasmine \"e2e/**/*.spec.ts\"",
    "db-admin": "DYNAMO_ENDPOINT=http://localhost:4566 dynamodb-admin",
    "create-queue": "AWS_ACCESS_KEY_ID=foobar AWS_SECRET_ACCESS_KEY=foobar aws --endpoint-url=http://localhost:4566 sqs create-queue --queue-name pathofchild-dev",
    "create-bucket": "AWS_ACCESS_KEY_ID=foobar AWS_SECRET_ACCESS_KEY=foobar aws --endpoint-url=http://localhost:4566 s3 mb s3://pathofchild-dev"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.12.0",
    "@graphql-tools/merge": "^6.2.11",
    "apollo-server-lambda": "^2.22.2",
    "aws-sdk": "^2.879.0",
    "bluebird": "^3.7.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^7.0.4",
    "jasmine": "^3.7.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nanoid": "^3.1.12",
    "puppeteer": "^5.5.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.6"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.200.0",
    "@serverless/aws": "^0.0.24",
    "@types/aws-lambda": "^8.10.17",
    "@types/jasmine": "^3.6.9",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@types/serverless": "^1.72.5",
    "apollo-server-testing": "^2.23.0",
    "cors": "^2.8.5",
    "eslint": "^7.25.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "serverless-localstack": "^0.4.30",
    "serverless-offline": "^6.9.0",
    "serverless-offline-dynamodb-streams": "4.1.0",
    "serverless-offline-sqs-external": "^1.0.6",
    "serverless-plugin-offline-dynamodb-stream": "^1.0.20",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  }
}
