{
  "name": "pathofchild-serverless",
  "private": true,
  "license": "WTFPL",
  "scripts": {
    "start": "cross-env AWS_REGION=eu-west-2 NODE_ENV=dev serverless offline start --dontPrintOutput --stage dev --region eu-west-2",
    "load": "ts-node ./lib/commands/load.ts",
    "save": "ts-node ./lib/commands/save.ts",
    "repopulate": "ts-node ./lib/commands/repopulate.ts",
    "migrate": "ts-node ./lib/commands/migrate.ts",
    "purify": "ts-node ./lib/commands/purify.ts",
    "graphqli": "cross-env NODE_ENV=dev ./node_modules/.bin/ts-node ./lib/graphqli/index.ts",
    "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "test": "ts-node node_modules/jasmine/bin/jasmine \"lib/**/*.spec.ts\"",
    "e2e": "cross-env NODE_ENV=test ts-node node_modules/jasmine/bin/jasmine \"e2e/**/*.spec.ts\"",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production"
  },
  "dependencies": {
    "@fluffy-spoon/substitute": "^1.145.0",
    "@graphql-tools/merge": "^6.2.1",
    "@types/express": "^4.17.6",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/jasmine": "^3.5.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.13.9",
    "apollo-server-lambda": "^2.17.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.747.0",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dynamoose": "^2.2.1",
    "elasticsearch": "^16.7.1",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^6.2.1",
    "jasmine": "^3.6.1",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.9.0",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.12",
    "serverless-offline": "^6.7.0",
    "serverless-offline-dynamodb-streams": "4.1.0",
    "serverless-plugin-offline-dynamodb-stream": "^1.0.19",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
