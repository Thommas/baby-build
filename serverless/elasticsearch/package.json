{
  "name": "pathofchild-elasticsearch",
  "version": "0.1.0",
  "license": "WTFPL",
  "scripts": {
    "start": "serverless offline start --stage dev --region eu-west-2",
    "start:local": "serverless offline start --stage local --region eu-west-2",
    "repopulate": "./node_modules/.bin/ts-node ./lib/repopulate/index.ts",
    "repopulate:local": "cross-env NODE_ENV=local ./node_modules/.bin/ts-node ./lib/repopulate/index.ts",
    "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "test": "jasmine-ts \"lib/**/*.spec.ts\"",
    "e2e": "jasmine-ts \"e2e/**/*.spec.ts\"",
    "lint": "tslint -c tslint.json './src/**/*.ts'",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production"
  },
  "dependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^7.0.5",
    "aws-sdk": "^2.205.0",
    "bluebird": "^3.7.1",
    "dotenv": "^5.0.1",
    "elasticsearch": "^15.2.0",
    "jasmine": "^3.1.0",
    "jasmine-ts": "^0.2.1",
    "lodash": "^4.17.14",
    "serverless-offline": "5.7.2",
    "serverless-offline-dynamodb-streams": "2.0.0",
    "serverless-plugin-offline-dynamodb-stream": "^1.0.18",
    "serverless-plugin-typescript": "^1.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
