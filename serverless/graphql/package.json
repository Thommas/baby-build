{
  "name": "pathofchild-graphql",
  "version": "0.6.0",
  "license": "WTFPL",
  "scripts": {
    "start": "serverless offline start --stage dev --region eu-west-2",
    "start:local": "cross-env NODE_ENV=local serverless offline start --stage dev --region eu-west-2",
    "puppeteer": "./node_modules/.bin/ts-node ./lib/puppeteer/index.ts",
    "fixtures": "./node_modules/.bin/ts-node ./lib/fixtures/index.ts",
    "fixtures:local": "cross-env NODE_ENV=local ./node_modules/.bin/ts-node ./lib/fixtures/index.ts",
    "graphqli": "./node_modules/.bin/ts-node ./lib/graphqli/index.ts",
    "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "lint": "tslint -c tslint.json './src/**/*.ts'",
    "test": "jasmine-ts \"lib/**/*.spec.ts\"",
    "e2e": "jasmine-ts \"e2e/**/*.spec.ts\"",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/express-graphql": "0.6.2",
    "@types/graphql": "^14.0.4",
    "@types/jasmine": "^3.3.5",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.12.18",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.387.0",
    "bluebird": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "dynamoose": "^1.3.1",
    "elasticsearch": "^15.3.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "14.4.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-server-lambda": "^1.4.0",
    "graphql-tools": "^4.0.3",
    "jasmine": "^3.3.1",
    "jasmine-ts": "^0.3.0",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.3.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "nanoid": "^2.0.0",
    "puppeteer": "^1.15.0",
    "serverless-offline": "^3.32.2",
    "serverless-plugin-typescript": "^1.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
