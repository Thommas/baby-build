{
  "name": "pathofchild-data",
  "private": true,
  "license": "WTFPL",
  "scripts": {
    "start": "ts-node ./src/load.ts",
    "save": "ts-node ./src/save.ts",
    "repopulate": "ts-node ./src/repopulate.ts",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint --fix --ext .ts src",
    "test": "cross-env NODE_ENV=test ts-node node_modules/jasmine/bin/jasmine \"src/**/*.spec.ts\"",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test"
  },
  "dependencies": {
    "@fluffy-spoon/substitute": "^1.130.0",
    "@hapi/joi": "^17.1.1",
    "@types/jasmine": "^3.5.10",
    "@types/node": "^13.13.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.658.0",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dynamoose": "^1.11.1",
    "elasticsearch": "^16.7.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jasmine": "^3.5.0",
    "lodash": "^4.17.15",
    "nanoid": "^3.1.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "globals": {},
    "rules": {
      "no-unused-vars": "warn"
    }
  }
}
